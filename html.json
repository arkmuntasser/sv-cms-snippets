{
  "svasset": {
    "prefix": "svasset",
    "body": [
      "{{asset.resource.getThumbnailUrl({",
      "\twidth: ${1:800}",
      "\theight: ${2:600}",
      "\tcrop: '${3:fill}'",
      "\tquality: ${4:60}",
      "\tfetch_format: '${5:jpg}'",
      "\tsv: { gravity: 'focalPoint' }",
      "})}}"
    ],
    "description": "Get the url for an asset"
  },
  "svimg": {
    "prefix": "svimg",
    "body": [
      "<img",
      "\tclass=\"${1:sv-img}\"",
      "\tsrc=\"{{asset.resource.getThumbnailUrl({",
      "\t\twidth: ${2:800}",
      "\t\theight: ${3:600}",
      "\t\tcrop: '${4:fill}'",
      "\t\tquality: ${5:60}",
      "\t\tfetch_format: '${6:jpg}'",
      "\t\tsv: { gravity: 'focalPoint' }",
      "\t})}}\"",
      "\talt=\"{{%asset.alt_text}}\"",
      "/>"
    ],
    "description": "An <img> loading an asset"
  },
  "svsource": {
    "prefix": "svsource",
    "body": [
      "<source",
      "\tsrc=\"{{asset.resource.getThumbnailUrl({",
      "\t\twidth: ${2:800}",
      "\t\theight: ${3:600}",
      "\t\tcrop: '${4:fill}'",
      "\t\tquality: ${5:60}",
      "\t\tfetch_format: '${6:jpg}'",
      "\t\tsv: { gravity: 'focalPoint' }",
      "\t})}}\"",
      "\tmedia=\"(min-width: ${7:641px})\"",
      "/>"
    ],
    "description": "An <img> loading an asset"
  },
  "svpicture": {
    "prefix": "svpicture",
    "body": [
      "<picture>",

      "\t<source",
      "\t\tsrcset=\"{{asset.resource.getThumbnailUrl({",
      "\t\t\twidth: ${2:800}",
      "\t\t\theight: ${3:600}",
      "\t\t\tcrop: '${4:fill}'",
      "\t\t\tquality: ${5:60}",
      "\t\t\tfetch_format: '${6:jpg}'",
      "\t\t\tsv: { gravity: 'focalPoint' }",
      "\t\t})}}\"",
      "\t\tmedia=\"(min-width: ${7:641px})\"",
      "\t/>",

      "\t<img",
      "\t\tclass=\"${8:sv-img}\"",
      "\t\tsrc=\"{{asset.resource.getThumbnailUrl({",
      "\t\t\twidth: ${9:800}",
      "\t\t\theight: ${10:600}",
      "\t\t\tcrop: '${11:fill}'",
      "\t\t\tquality: ${12:60}",
      "\t\t\tfetch_format: '${13:jpg}'",
      "\t\t\tsv: { gravity: 'focalPoint' }",
      "\t\t})}}\"",
      "\t\talt=\"{{%asset.alt_text}}\"",
      "\t/>",
      "</picture>"
    ],
    "description": "An <picture> loading an asset"
  },
  "svcollection": {
    "prefix": "svcollection",
    "body": [
      "<!--SVMETA",
      "{",
      "\t\"requireBundles\" : [",
      "\t\t\"widget_plugins_collections_template_custom_collection\",",
      "\t\t\"plugins_collections_shared\"",
      "\t],",
      "\t\"partials\" : [",
      "\t\t{ \"plugin\" : \"collections\", \"name\" : \"custom_collection_helper\" },",
      "\t\t{ \"plugin\" : \"common\", \"name\" : \"custom_play_button\" }",
      "\t]",
      "}",
      "-->",
      "",
      "{{>custom_collection_helper}}",
      "",
      "{{+slide}}",
      "\t<div class=\"inner\">",
      "\t\t<div class=\"img-cont\">",
      "\t\t\t<a data-asset href=\"{{assetLink()}}\" target=\"{{link.target}}\">",
      "\t\t\t\t<picture>",
      "\t\t\t\t\t<img",
      "\t\t\t\t\t\tsrc=\"{{asset.resource.getThumbnailUrl({",
      "\t\t\t\t\t\t\twidth: 800",
      "\t\t\t\t\t\t\theight: 600",
      "\t\t\t\t\t\t\tcrop: 'fill'",
      "\t\t\t\t\t\t\tquality: 60",
      "\t\t\t\t\t\t\tfetch_format: 'jpg'",
      "\t\t\t\t\t\t\tsv: { gravity: 'focalPoint' }",
      "\t\t\t\t\t\t})}}\"",
      "\t\t\t\t\t\talt=\"{{%asset.alt_text}}\"",
      "\t\t\t\t\t/>",
      "\t\t\t\t</picture>",
      "\t\t\t</a>",
      "\t\t\t{{:isVideo()}}{{>custom_play_button}}{{/}}",
      "\t\t</div>",
      "\t\t<div class=\"content-section\">",
      "\t\t\t<h4><a href=\"{{link.url}}\" target=\"{{link.target}}\">{{title}}</a></h4>",
      "\t\t\t{{:description}}<p>{{plainDescription(100)}}</p>{{/}}",
      "\t\t\t{{:link}}",
      "\t\t\t\t<a",
      "\t\t\t\t\tclass=\"read-more\"",
      "\t\t\t\t\thref=\"{{link.url}}\"",
      "\t\t\t\t\ttarget=\"{{link.target}}\"",
      "\t\t\t\t>",
      "\t\t\t\t\t{{*templateData.more_caption}} <i class=\"fas fa-arrow-right\" aria-hidden=\"true\"></i>",
      "\t\t\t\t</a>",
      "\t\t\t{{/}}",
      "\t\t</div>",
      "\t</div>",
      "{{/}}",
      "",
      "<div",
      "\tclass=\"collection\"",
      "\tdata-guid=\"{{guid}}\"",
      "\tdata-template-data=\"{{%%~stringify(data.templateData || {})}}\"",
      "\tdata-model-name=\"{{modelName}}\"",
      ">",
      "\t{{:templateData.adunits}}<script type=\"text/template\" data-slide-template>{{~partial('slide')}}</script>{{/}}",
      "\t<div class=\"cols\" data-slides>",
      "\t\t{{#items}}",
      "\t\t\t<div class=\"slide {{:isDTN}}dtn-placeholder{{/}}\">",
      "\t\t\t\t{{>slide}}",
      "\t\t\t</div>",
      "\t\t{{/}}",
      "\t</div>",
      "</div>",
      "",
      "<script>",
      "\trequire([",
      "\t\t'jquery',",
      "\t\t'plugins_collections_custom_collection_helper',",
      "\t], function($, collectionHelper) {",
      "\t\tvar root = $('[data-guid=\"{{guid}}\"]');",
      "\t\tcollectionHelper.init(root);",
      "\t});",
      "</script>"
    ],
    "description": "Template for a generic collection widget"
  }
}
